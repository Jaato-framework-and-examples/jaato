{
  "name": "custom-example",
  "description": "A starting point for creating your own theme. Edit the colors below.",
  "version": "1.0",
  "colors": {
    "primary": "#61afef",
    "secondary": "#98c379",
    "accent": "#e5c07b",
    "success": "#98c379",
    "warning": "#e5c07b",
    "error": "#e06c75",
    "muted": "#5c6370",
    "background": "#282c34",
    "surface": "#3e4451",
    "text": "#abb2bf",
    "text_muted": "#5c6370"
  },
  "semantic": {
    "agent_tab_selected": {"fg": "primary", "bold": true, "underline": true},
    "agent_tab_dim": {"fg": "muted"},

    "model_header": {"fg": "primary", "bold": true},
    "user_header": {"fg": "secondary", "bold": true},

    "tool_output": {"fg": "#56b6c2", "italic": true},
    "tool_success": {"fg": "success"},
    "tool_error": {"fg": "error", "dim": true},

    "permission_prompt": {"fg": "warning", "bold": true},

    "spinner": {"fg": "primary"},
